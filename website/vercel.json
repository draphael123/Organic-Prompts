{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/prompts",
      "destination": "/prompts.html"
    },
    {
      "source": "/guides",
      "destination": "/guides.html"
    },
    {
      "source": "/examples",
      "destination": "/examples.html"
    },
    {
      "source": "/strategy",
      "destination": "/strategy.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

